<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="TestEntity">

    <insert id="insert">
        INSERT INTO TestEntity
        (
        AddTime,
        value,
        date
        )
        VALUES
        (
        now(),
        #e.value#,
        #e.date#
        );
        <selectKey resultClass="java.lang.Integer" keyProperty="id">
            SELECT LAST_INSERT_ID()
        </selectKey>

    </insert>

    <delete id="delete">
        DELETE FROM TestEntity
        WHERE Id = #id#;
    </delete>


    <select id="get" resultClass="com.dianping.demo.entity.TestEntity">
        SELECT * FROM TestEntity
        WHERE Id = #id#;
    </select>

    <update id="update">
        UPDATE  TestEntity
        SET
        value = #e.value#,
        date = #e.date#
        WHERE Id = #id#;
    </update>

</sqlMap>